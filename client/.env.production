# Production Environment Variables for React App

# API Configuration (Will be set to your backend URL)
REACT_APP_API_URL=https://your-backend-app.onrender.com/api
REACT_APP_SOCKET_URL=https://your-backend-app.onrender.com

# QR Code Configuration
REACT_APP_QR_BASE_URL=https://your-frontend-app.onrender.com/check

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_DARK_MODE=false
